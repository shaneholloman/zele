{
  "name": "zele",
  "version": "0.3.1",
  "description": "Gmail CLI â€” manage email from your terminal",
  "type": "module",
  "main": "dist/cli.js",
  "bin": "./dist/cli.js",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "pnpm build",
    "dev": "tsx src/cli.ts",
    "generate": "prisma generate && pnpm generate:sql",
    "generate:sql": "rm -f dev.db && prisma db push --url 'file:dev.db' --accept-data-loss && echo '-- Generated by pnpm generate:sql. Do not edit.' > src/schema.sql && sqlite3 dev.db '.schema' >> src/schema.sql",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "gmail",
    "cli",
    "email"
  ],
  "author": "remorses",
  "license": "ISC",
  "packageManager": "pnpm@10.18.1",
  "dependencies": {
    "@googleapis/gmail": "^16.1.1",
    "@libsql/client": "^0.15.15",
    "@prisma/adapter-libsql": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "email-addresses": "^5.0.0",
    "fkill": "^10.0.3",
    "goke": "^6.1.3",
    "google-auth-library": "^10.5.0",
    "js-yaml": "^4.1.1",
    "mimetext": "^3.0.28",
    "picocolors": "^1.1.1",
    "ts-ics": "^2.4.0",
    "tsdav": "^2.1.7",
    "turndown": "^7.2.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/email-addresses": "^3.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.2.0",
    "@types/turndown": "^5.0.5",
    "prisma": "^7.3.0",
    "tsx": "^4.19.4",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/engines",
      "prisma"
    ]
  }
}
